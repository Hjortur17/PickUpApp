{
  "expo": {
    "name": "Geysir Car Rental | Pick-up app",
    "slug": "geysir-car-rental-pick-up-app",
    "description": "App used by Geysir Car Rental assist customers picking up from a contactless key pick-up",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C23A2C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#C23A2C"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
